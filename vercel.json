{
  "version": 2,
  "functions": {
    "api/**/*.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "^/$", "dest": "/api/index.js" },
    { "src": "^/auth$", "dest": "/api/auth.js" },
    { "src": "^/callback$", "dest": "/api/callback.js" },
    { "src": "^/success$", "dest": "/api/success.js" }
  ]
}
